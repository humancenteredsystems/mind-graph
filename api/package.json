{
  "name": "api",
  "version": "1.0.0",
  "description": "Backend API for MakeItMakeSense.io graph platform",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon server.ts",
    "test": "jest",
    "test:verbose": "VERBOSE_TESTS=true jest",
    "test:watch": "jest --watch",
    "test:watch:verbose": "VERBOSE_TESTS=true jest --watch",
    "test:failed": "jest --onlyFailures",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:integration-real": "jest --testPathPattern=integration-real",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "Human-Centered Systems, LLC",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "ssh2": "^1.16.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.24",
    "@types/ssh2": "^1.15.5",
    "@types/supertest": "6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "eslint": "^9.28.0",
    "eslint-plugin-enterprise": "file:../eslint-plugin-enterprise",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
